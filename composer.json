{
    "name": "tigusigalpa/yandexgpt-php",
    "description": "PHP SDK для работы с YandexGPT API с поддержкой Laravel",
    "type": "library",
    "keywords": [
        "yandex",
        "gpt",
        "ai",
        "laravel",
        "php",
        "sdk",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Sazonov",
            "email": "sovletig@gmail.com"
        }
    ],
    "homepage": "https://github.com/tigusigalpa/yandexgpt-php",
    "support": {
        "issues": "https://github.com/tigusigalpa/yandexgpt-php/issues",
        "source": "https://github.com/tigusigalpa/yandexgpt-php"
    },
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Tigusigalpa\\YandexGPT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tigusigalpa\\YandexGPT\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tigusigalpa\\YandexGPT\\Laravel\\YandexGPTServiceProvider"
            ],
            "aliases": {
                "YandexGPT": "Tigusigalpa\\YandexGPT\\Laravel\\Facades\\YandexGPT"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
